{
  "aggregate": "s3-tpch-q5v2-customer",
  "collection": "customer",
  "pipeline": [
    {
      "$lookup": {
        "from": "orders-lineitem",
        "localField": "_id",
        "foreignField": "o_custkey",
        "as": "orders"
      }
    },
    {
      "$project": {
        "_id": 1,
        "orders": {
          "$filter": {
            "input": "$orders",
            "as": "order",
            "cond": {
              "gt": [
                "$$order.o_totalprice",
                37500
              ]
            }
          }
        }
      }
    },
    {
      "$unwind": "$orders"
    },
    {
      "$project": {
        "_id": 1,
        "orderkey": "$orders._id"
      }
    }
  ],
  "allowDiskUse": true,
  "maxTimeMS": 0,
  "cursor": {}
}