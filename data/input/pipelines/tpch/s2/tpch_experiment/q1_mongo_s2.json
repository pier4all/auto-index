{
  "aggregate": "s2-tpch-q1-lineitem",
  "collection": "lineitem",
  "pipeline": [
    {
      "$match": {
        "l_shipdate": {
          "$lte": "1992-01-02T00:00:00Z"
        }
      }
    },
    {
      "$group": {
        "_id": {
          "l_returnflag": "$l_returnflag",
          "l_linestatus": "$l_linestatus"
        },
        "sum_qty": {
          "$sum": "$l_quantity"
        },
        "sum_base_price": {
          "$sum": "$l_extendedprice"
        },
        "sum_disc_price": {
          "$sum": {
            "$multiply": [
              "$l_extendedprice",
              {
                "$subtract": [
                  1,
                  "$l_discount"
                ]
              }
            ]
          }
        },
        "sum_charge": {
          "$sum": {
            "$multiply": [
              "$l_extendedprice",
              {
                "$subtract": [
                  1,
                  "$l_discount"
                ]
              },
              {
                "$add": [
                  1,
                  "$l_tax"
                ]
              }
            ]
          }
        },
        "avg_qty": {
          "$avg": "$l_quantity"
        },
        "avg_price": {
          "$avg": "$l_extendedprice"
        },
        "avg_disc": {
          "$avg": "$l_discount"
        },
        "count_order": {
          "$sum": 1
        }
      }
    },
    {
      "$sort": {
        "_id.l_returnflag": 1,
        "_id.l_linestatus": 1
      }
    }
  ],
  "allowDiskUse": true,
  "maxTimeMS": 0,
  "cursor": {}
}