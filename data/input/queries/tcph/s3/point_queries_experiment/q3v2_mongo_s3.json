{
  "aggregate": "customer",
  "pipeline": [
    {
      "$lookup": {
        "from": "orders-lineitem",
        "localField": "_id",
        "foreignField": "o_custkey",
        "as": "orders"
      }
    },
    {
      "$unwind": "$orders"
    },
    {
      "$project": {
        "c_name": 1,
        "o_orderdate": "$orders.o_orderdate"
      }
    }
  ],
  "allowDiskUse": true,
  "maxTimeMS": 0,
  "cursor": {}
}