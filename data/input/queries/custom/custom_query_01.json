{
    "aggregate":"custom-01",
    "pipeline":[
      {
        "$match":{
          "color": "red"
        }
      },
      {
        "$sort":{
          "shape":1,
          "size":1,
          "color": -1
        }
      },{
        "$match":{
          "round": false
        }
      }
    ],
    "collection": "figures",
    "allowDiskUse":true,
    "maxTimeMS":0,
    "cursor":{}
}